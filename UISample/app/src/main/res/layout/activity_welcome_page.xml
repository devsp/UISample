<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.sanjanapillai.uisample.WelcomePage"
    >

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/logo2"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />



   <ImageButton
       android:layout_height="100dp"
       android:layout_width="90dp"
       android:src="@drawable/booktrip"
       android:background="@android:color/transparent"
       android:scaleType="fitXY"
       android:onClick="booktrip"
       android:layout_alignTop="@+id/btnevent"
       android:layout_toStartOf="@+id/imageView"
       android:id="@+id/btnbookatrip" />



    <ImageButton
        android:layout_height="100dp"
        android:layout_width="90dp"
        android:src="@drawable/top"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:id="@+id/btnevent"
        android:onClick="event"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/imageView"
        android:layout_toEndOf="@+id/imageView" />

    <ImageButton
        android:layout_height="100dp"
        android:layout_width="90dp"
        android:src="@drawable/event"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_marginTop="34dp"
        android:id="@+id/imageButton4"
        android:layout_below="@+id/btnbookatrip"
        android:layout_alignStart="@+id/btnbookatrip" />

    <ImageButton
        android:layout_height="100dp"
        android:layout_width="90dp"
        android:src="@drawable/search"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignStart="@+id/btnevent"
        android:id="@+id/imageButton5" />

    <ImageButton
        android:layout_height="100dp"
        android:layout_width="90dp"
        android:src="@drawable/group"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageButton4"
        android:layout_marginBottom="28dp"
        android:id="@+id/imageButton6" />

    <ImageButton
        android:layout_height="100dp"
        android:layout_width="90dp"
        android:src="@drawable/rate"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton5" />


    <ImageButton
        android:layout_height="50dp"
        android:layout_width="50dp"
        android:src="@drawable/user2"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageButton12" />


</RelativeLayout>
